{
  "name": "ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ui --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "vue-tsc --noEmit && prettier --check \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "lint-format": "vue-tsc --noEmit && eslint ui --fix && prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\" && knip --no-exit-code",
    "check-unused": "knip",
    "check-unused:fix": "knip --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "bootstrap": "^5.3.5",
    "chart.js": "^4.4.9",
    "vue": "^3.5.16",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.2.0",
    "knip": "^5.61.0",
    "prettier": "^3.5.3",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
