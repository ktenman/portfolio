{
  "name": "ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ui --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "vue-tsc --noEmit && prettier --check \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "lint-format": "vue-tsc --noEmit && eslint ui --fix && prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\" && knip && ./gradlew ktlintFormat detekt",
    "ktlint": "./gradlew ktlintCheck",
    "ktlint:fix": "./gradlew ktlintFormat",
    "detekt": "./gradlew detekt",
    "check-unused": "knip",
    "check-unused:fix": "knip --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:intellij": "vitest run --coverage --exclude='**/confirm-dialog.test.ts' && chmod +x fix-coverage.sh && ./fix-coverage.sh"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.90.5",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.1",
    "lucide-vue-next": "^0.548.0",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-comments": "^1.1.10",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "knip": "^5.66.4",
    "prettier": "^3.6.2",
    "sass": "1.70.0",
    "typescript": "5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6",
    "vue-tsc": "^3.1.2"
  }
}
