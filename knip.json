{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["ui/main.ts", "ui/index.html"],
  "project": ["ui/**/*.{ts,tsx,vue,js}"],
  "rules": {
    "types": "off"
  },
  "ignore": [
    "ui/public/**",
    "ui/vite-env.d.ts",
    "ui/**/*.d.ts",
    "ui/tests/setup.ts",
    "ui/models/generated/**",
    "src/**",
    "build/**",
    "dist/**",
    "k8s/**",
    "cloudflare-bypass-proxy/**"
  ],
  "ignoreUnresolved": ["./tests/setup.ts"],
  "ignoreDependencies": ["vue-eslint-parser", "espree", "eslint-plugin-no-comments"],
  "ignoreExportsUsedInFile": false,
  "includeEntryExports": true,
  "vue": {
    "compiler": "vue-tsc"
  },
  "vite": {
    "config": ["vite.config.ts"]
  },
  "vitest": {
    "config": ["vite.config.ts"],
    "entry": ["ui/**/*.test.ts"]
  },
  "typescript": {
    "config": ["tsconfig.json", "tsconfig.node.json"]
  },
  "eslint": {
    "config": ["eslint.config.js"]
  },
  "ignoreBinaries": [],
  "ignoreWorkspaces": []
}
